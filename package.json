    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.11.0",
    "tslint-eslint-rules-recommended": "^1.2.2",
    "typescript": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
