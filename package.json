{
  "name": "taulukko-commons",
  "version": "1.0.6",
  "type": "commonjs",
  "description": "Taulukko Commons Library",
  "private": false,
  "author": "Taulukko Inc.",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
